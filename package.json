{
  "name": "ta-app",
  "version": "1.0.0",
  "description": "Trucked Away with Express Server",
  "main": "index.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node index.js",
    "server": " nodemon index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Trucked Away",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "stripe": "^7.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
